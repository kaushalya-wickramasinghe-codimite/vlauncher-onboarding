{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=vlauncher;User=YOUR_DB_USER;Password=YOUR_DB_PASSWORD;"
  },
  "ActiveDirectory": {
    "Server": "YOUR_AD_SERVER_IP",
