    "AdminPassword": "YOUR_AD_ADMIN_PASSWORD",
    "AdminGroupName": "VLauncher-Admins",
    "SecurityGroupsOu": "OU=VLauncher,DC=YOUR_DOMAIN,DC=local",
    "UsersOu": "OU=VLauncher,DC=YOUR_DOMAIN,DC=local"
  }
}
